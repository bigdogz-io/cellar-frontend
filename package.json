    "vue-i18n": "^8.22.2",
    "vue-media-queries": "0.0.5",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@pact-foundation/pact": "^9.13.0",
    "@vue/cli": "~4.5.9",
    "@vue/cli-plugin-babel": "~4.5.0",
